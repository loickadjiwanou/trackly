{
  "expo": {
    "name": "trackly",
    "slug": "trackly",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/piks/png/3d-mobile-shopping-concept-with-delivery-boxes-shopping-bag.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./src/assets/default/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "android": {
      "splash": {
        "resizeMode": "cover",
        "backgroundColor": "#ffffff"
      },
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/default/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "ios": {
      "splash": {
        "resizeMode": "cover",
        "backgroundColor": "#ffffff"
      },
      "supportsTablet": true
    },
    "web": {
      "favicon": "./src/assets/default/favicon.png"
    }
  }
}
